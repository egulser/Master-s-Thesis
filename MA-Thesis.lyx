reference "fig:Changes-in-Real-Decile"
plural "false"
caps "false"
noprefix "false"

\end_inset

 below:
\begin_inset Formula 
\begin{eqnarray}
\widehat{\Delta wage} & = & \underset{(0.103)^{***}}{-0.978}logwage(2004)+\underset{(.019)^{***}}{.175}logwage(2004)^{2}+\underset{(0.135)^{***}}{1.548},\label{eq:quadeq}
\end{eqnarray}

