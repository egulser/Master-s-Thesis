@article{2002Card,
	abstract = {The recent rise in wage inequality is usually attributed to skill&#x2010;biased technical change (SBTC), associated with new computer technologies. We review the evidence for this hypothesis, focusing on the implications of SBTC for overall wage inequality and for changes in wage differentials between groups. A key problem for the SBTC hypothesis is that wage inequality stabilized in the 1990s despite continuing advances in computer technology; SBTC also fails to explain the evolution of other dimensions of wage inequality, including the gender and racial wage gaps and the age gradient in the return to education.},
	author = {Card, David and DiNardo, John&#xa0 and E.},
	database = {JSTOR},
